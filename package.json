{
  "name": "react-isomorphic",
  "version": "3.0.0",
  "author": "Vladimir Zhelvis",
  "license": "MIT",
  "description": "env for build react isomorphic apps",
  "main": "server.js",
  "scripts": {
    "check-types": "tsc --noEmit",
    "dev": "cross-env NODE_ENV=development webpack --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "lint": "eslint --fix src/**/*",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "bugs": {
    "url": "https://gitlab.com/webdevtemp/base-js/issues"
  },
  "homepage": "https://gitlab.com/webdevtemp/base-js#readme",
  "dependencies": {
    "@loadable/component": "5.14.1",
    "@loadable/server": "5.14.2",
    "core-js": "3.9.1",
    "express": "4.17.1",
    "history": "5.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-router": "6.0.0-beta.0",
    "react-router-dom": "6.0.0-beta.0"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/webpack-plugin": "5.14.2",
    "@types/express": "4.17.11",
    "@types/loadable__component": "5.13.3",
    "@types/loadable__server": "5.12.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "@welldone-software/why-did-you-render": "6.1.1",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-webpack-plugin": "2.5.2",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "nodemon-webpack-plugin": "4.4.4",
    "prettier": "2.2.1",
    "typescript": "4.2.3",
    "webpack": "5.25.0",
    "webpack-cli": "4.5.0",
    "webpack-node-externals": "2.5.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "node_modules/.bin/eslint --fix --max-warnings=0 src/**/*"
    ]
  }
}
